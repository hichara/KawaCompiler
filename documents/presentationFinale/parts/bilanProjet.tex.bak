    	 \subsection{Les difficultés rencontrées}
	    \begin{frame}{Les difficultés rencontrées}{}
	    	\begin{block}{L'équipe}
	    		\begin{itemize}
					\item<1-> Abandon d'un membre du groupe 
					\item<2-> Chef de projet contraint de déléguer la tache par soucis personnel
					\item<3-> Membre absent durant une période critique
	    		\end{itemize}
	    	\end{block}
	    \end{frame}

	    \begin{frame}{Les difficultés rencontrées}{}
	    	\begin{block}{Le projet et sa gestion}
	    		\begin{itemize}
					\item<1-> Un projet difficile à appréhender et à prendre en main
					\item<2-> Documents difficiles à écrire à cause de cette complexité du projet
					\item<3-> Un langage non maîtrisé
					\item<4-> API LLVM inconnu et aucun référent 
	    		\end{itemize}
	    	\end{block}
	    \end{frame}

	\subsection{Erreurs commises}
	    \begin{frame}{Erreurs commises}{}
	    	\begin{block}{L'équipe}
	    		\begin{itemize}
					\item<1-> Pas assez d'inquiétude malgré l'absence d'un membre
						\begin{itemize}
							\item<2-> Perte de temps pour remettre sur pieds un projet avec une personne de moins
						\end{itemize}
					\item<3-> Communication difficile
	    		\end{itemize}
	    	\end{block}
	    \end{frame}

	    \begin{frame}{Erreurs commises}{}
	    	\begin{block}{Le projet et sa gestion}
	    		\begin{itemize}
					\item<1-> Mauvaise évaluation des charges de travail (manque d'expérience)
					\item<2-> Des documents pas assez spécifiés, incomplets au lancement du projet
					\item<3-> Module géré par une seule personne 
					\begin{itemize}
						\item<3-> Risque de SPLOF
					\end{itemize}
					\item<4-> Pas de mise en place itérative pour atteindre les objectifs
					\begin{itemize}
						\item<4-> Mauvaise gestion des étapes, des jalons, des sous jalons
					\end{itemize} 
					\item<5-> PDD non respecté, non mis à jour, trop peu détaillé
					\item<6-> Intégration
	    		\end{itemize}
	    	\end{block}
	    \end{frame}

